    {
        "id": "dc753bae14790aa6",
        "type": "function",
        "z": "164dcb0874348152",
        "name": "Status",
        "func": "var temperatureData = msg.payload[0];\nvar winStatus = flow.get('ventanaAbierta');\n\nif (temperatureData) {\n    var temperature = temperatureData._value;\n    msg.payload = {\n        chatId: msg.chatId,\n        type: \"message\",\n        content: \"Temperatura actual: \" + temperature + \"°C | Ventanas: \" + (winStatus ? \"abiertas\" : \"cerradas\"),\n        options: {\n            chat_id: msg.chatId,\n            text: \"Temperatura actual: \" + temperature + \"°C | Ventanas: \" + (winStatus ? \"abiertas\" : \"cerradas\"),\n        }\n    };\n} else {\n    msg.payload = {\n        chatId: msg.chatId,\n        type: \"message\",\n        content: \"No hay datos disponibles de temperatura.\",\n        options: {\n            chat_id: msg.chatId,\n            text: \"No hay datos disponibles de temperatura.\"\n        }\n    };\n}\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1330,
        "y": 740,
        "wires": [
