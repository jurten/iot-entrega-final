    {
        "id": "2eada13525b6e10a",
        "type": "function",
        "z": "164dcb0874348152",
        "name": "Map temp to message",
        "func": "var chatIds = flow.get('chatIds') || [];\nvar winStatus = flow.get('ventanaAbierta');\n\nvar temperature = msg.payload.value;\nvar messages = [];\n\n// Prepare messages for each chat ID in chatIds array\nchatIds.forEach(function (chatId) {\n    messages.push({\n        payload: {\n            chatId: chatId,\n            type: \"message\",\n            content: \"Temperatura actual: \" + temperature + \"°C | Ventanas: \" + (winStatus ? \"abiertas\" : \"cerradas\"),\n            options: {\n                chat_id: chatId,\n                text: \"Temperatura actual: \" + temperature + \"°C | Ventanas: \" + (winStatus ? \"abiertas\" : \"cerradas\"),\n            }\n        }\n    });\n});\n\n// Send messages array to output\nreturn [messages];",
